{
  "name": "fractale",
  "version": "2.8.2",
  "description": "Modeling architecture data",
  "main": "lib/index.js",
  "scripts": {
    "docs:build-api": "npx jsdoc2md --files lib/**/*.js > docs/api.md",
    "docs:prepare": "npx gitbook init",
    "docs:build": "npm run docs:build-api && npm run docs:prepare && npx gitbook build",
    "docs:start": "npm run docs:prepare && npm run docs:build-api && npx gitbook serve",

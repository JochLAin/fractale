  "license": "SEE LICENSE IN LICENSE",
  "homepage": "http://docs.faihy.org/fractale",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "uuid": "^3.3.3"
  },
  "peerDependencies": {
    "moment": "^2.24.0",
    "mongoose": "^5.8.1",
    "prop-types": "^15.7.2",
    "teinte": "^1.0.1"
  },
  "devDependencies": {
    "chart.js": "^2.9.3",
    "chartjs-node": "^1.7.1",
    "cli-progress": "^3.4.0",
    "crieur": "^1.3.0",
    "object-sizeof": "^1.5.2"
  }
